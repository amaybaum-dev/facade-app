{
  "name": "facade-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:80",
  "dependencies": {
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "run": "json5",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test -u",
    "eject": "react-scripts eject",
    "pretty": "prettier --write \"./**/*.{js,jsx,tsx,css}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
        "<rootDir>/src/**/*.{js,ts,jsx,tsx}",
        "!<rootDir>/src/test/**",
        "!<rootDir>/src/interface/*",
        "!<rootDir>/src/react-app-env.d.ts"
    ]
  },
  "devDependencies": {
    "ts-node": "^10.7.0"
  }
}
